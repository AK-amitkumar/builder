{%- extends 'xml/base.xml.jinja2' %}
{% import 'macros.jinja2' as macros %}

{% block content %}

    <template id="themes" inherit_id="website.themes" name="Themes">
            <xpath expr="//div[@id='themes-list']" position="inside">
                {% for theme in themes %}
                {{ macros.builder_website_theme(theme) }}
                {% endfor %}
            </xpath>
        </template>
{% endblock %}